  "reduplication": {
    "Ca reduplication": {
        "rule": "If the first letter of the second to last syllable in a stem is a consonant, reduplicate the consonant of the stem and add -a- right before the consonant. If the first letter of the second to last syllable in a stem is a vowel, directly add an -a- right before the vowel.",
        "meanings": [],
        "notes": "Most stems have two syllables, so usually the second to last syllable is the first syllable."
    },
    "CVCV- reduplication": {
        "rule": "Reduplicate the last two syllables of the stem. The consonants after the vowel in the latter syllable are ignored.",
        "meanings": [],
        "notes": ""
    }
  }
